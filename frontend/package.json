    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "mapbox-gl": "^3.17.0",
    "maplibre-gl": "^5.15.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-map-gl": "^8.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.11.0",
    "recharts": "^3.6.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
